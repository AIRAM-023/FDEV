{
	"comments": {
		"lineComment": "#",
		"blockComment": [ "<#", "#>" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		{ "open": "\"", "close": "\"", "notIn": ["string"]},
		{ "open": "'", "close": "'", "notIn": ["string", "comment"]},
		["<#", "#>"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*#[rR]egion\\b",
			"end": "^\\s*#[eE]nd[rR]egion\\b"
		}
	},
	"indentationRules": {
		"increaseIndentPattern": "^.*\\{[^}\"']*$|^.*\\([^)\"']*$|^\\s*\\{\\}$|^\\s*\\(\\)$",
		"decreaseIndentPattern": "^(.*\\*/)?\\s*[\\}\\)]([^}{\"']*[\\{\\(]|\\s*while\\s*\\(.*)?[;\\s]*(//.*|/\\*.*\\*/\\s*)?$"
	}
}