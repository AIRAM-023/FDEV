{
  "comments": {
    "lineComment": "#",
    "blockComment": [
      "#[[",
      "]]"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string"
      ]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": [
        "string"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "\\b(function|macro|if|foreach|while)\\b|\\belse(if)?\\b|\\([^)]*$",
    "decreaseIndentPattern": "\\bend(function|macro|if|foreach|while)\\b|\\belse(if)?\\b|^[^(]*\\)\\s*$",
  }
}