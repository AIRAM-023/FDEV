{
	"comments": {
		"lineComment": "//",
		"blockComment": [ "/*", "*/" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "`"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "`"]
	],
	"indentationRules": {
		"increaseIndentPattern": "(?!^\\s*\\/\\/)(\\{[^\\}\"']*(//.*)?$|\\([^\\{\\)\\}\"']*(//.*)?$|\\[[^\\(\\{\\)\\]\\}\"']*(//.*)?$|^\\s*case\\b|^\\s*default\\b)",
		"decreaseIndentPattern": "(?!^\\s*\\/\\/)(^[^\\{\"']*\\}|^[^\\(\\{\\}\"']*\\)|^[^\\(\\[\\{\\}\\)\"']*\\]|^\\s*case\\b|^\\s*default\\b)"
	}
}