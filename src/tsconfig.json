{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": false,
		"allowJs": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "../out/vs",
		"module": "ESNext",
		"types": [
			"mocha",
			"semver",
			"sinon",
			"winreg",
			"trusted-types",
			"wicg-file-system-access"
		],
		"plugins": [
			{
				"name": "tsec",
				"exemptionConfig": "./tsec.exemptions.json"
			}
		]
	},
	"include": [
		"bootstrap.cjs",
		"bootstrap-amd.cjs",
		"./server-main.js",
		"./typings",
		"./vs/**/*.ts",
		"vscode-dts/vscode.proposed.*.d.ts",
		"vscode-dts/vscode.d.ts",
		"vs/platform/environment/node/userDataPath.d.cts",
		"vs/base/common/stripComments.cjs",
		"vs/base/node/unc.js"
	]
}
