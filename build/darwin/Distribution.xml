<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
	<pkg-ref id="com.test.path.pkg">
		<bundle-version/>
	</pkg-ref>
	<pkg-ref id="com.test.workflow.pkg">
		<bundle-version/>
	</pkg-ref>
	<pkg-ref id="com.test.vscode.pkg">
		<bundle-version>
			<bundle CFBundleShortVersionString="1.0" CFBundleVersion="1" id="org.reactivecocoa.ReactiveCocoa" path="Code - OSS.app/Contents/Frameworks/ReactiveCocoa.framework"/>
			<bundle CFBundleShortVersionString="1.0" CFBundleVersion="" id="org.mantle.Mantle" path="Code - OSS.app/Contents/Frameworks/Mantle.framework"/>
			<bundle CFBundleShortVersionString="1.48.1" CFBundleVersion="1.48.1" id="com.microsoft.VSCode" path="Code - OSS.app"/>
			<bundle CFBundleShortVersionString="1.0" CFBundleVersion="1" id="com.github.Squirrel" path="Code - OSS.app/Contents/Frameworks/Squirrel.framework"/>
			<bundle CFBundleVersion="7.3.2" id="com.github.Electron.framework" path="Code - OSS.app/Contents/Frameworks/Electron Framework.framework"/>
			<bundle id="com.github.Electron.helper" path="Code - OSS.app/Contents/Frameworks/Code Helper (GPU).app"/>
			<bundle id="com.microsoft.VSCode.helper" path="Code - OSS.app/Contents/Frameworks/Code Helper.app"/>
		</bundle-version>
	</pkg-ref>
	<!-- Options are never, allow, or always -->
	<options customize="always" require-scripts="false"/>
	<choices-outline>
		<line choice="default">
			<line choice="com.test.path.pkg"/>
			<line choice="com.test.vscode.pkg"/>
			<line choice="com.test.workflow.pkg"/>
		</line>
	</choices-outline>
	<choice id="default" title="Code - OSS"/>
	<choice id="com.test.path.pkg" visible="true" title="Add to PATH" description="Add code to PATH">
		<pkg-ref id="com.test.path.pkg"/>
	</choice>
	<pkg-ref id="com.test.path.pkg" version="0" onConclusion="none" installKBytes="0">#path.pkg</pkg-ref>
	<choice id="com.test.workflow.pkg" visible="true" title="Add to right-click menu" description="Add 'Open in Code - OSS' to right-click menu">
		<pkg-ref id="com.test.workflow.pkg"/>
	</choice>
	<pkg-ref id="com.test.workflow.pkg" version="0" onConclusion="none" installKBytes="6">#workflow.pkg</pkg-ref>
	<choice id="com.test.vscode.pkg" visible="false">
		<pkg-ref id="com.test.vscode.pkg"/>
	</choice>
	<pkg-ref id="com.test.vscode.pkg" version="0" onConclusion="none" installKBytes="247964">#app.pkg</pkg-ref>
</installer-gui-script>
